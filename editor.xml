<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="IS_LEADING_WHITESPACES_SHOWN" value="false" />
    <option name="IS_INNER_WHITESPACES_SHOWN" value="false" />
    <option name="IS_TRAILING_WHITESPACES_SHOWN" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="73bf" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="73bf" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
    <pattern icon="question" useCustomColors="true" case-sensitive="false" pattern="INFO:">
      <option name="FOREGROUND" value="5bbf18" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
  </component>
  <component name="XmlEditorOptions">
    <option name="insertQuotesForAttributeValue" value="true" />
    <option name="showCssInlineColorPreview" value="true" />
    <option name="tagTreeHighlightingLevelCount" value="12" />
  </component>
</application>